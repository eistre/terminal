{
  "$schema": "node_modules/turbo/schema.json",
  "globalEnv": ["NODE_ENV", "LOG_LEVEL"],
  "globalDependencies": [".env*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nuxt/**", ".output/**", "dist/**", "build/**"]
    },
    "clean": {
      "cache": false,
      "dependsOn": ["^clean"]
    },
    "//#clean:root": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"],
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": [".cache/.eslintcache"]
    },
    "//#lint:root": {
      "outputs": [".cache/.eslintcache"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"],
      "outputs": [".cache/.eslintcache"]
    },
    "//#lint:root:fix": {
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    }
  }
}
