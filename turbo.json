{
  "$schema": "node_modules/turbo/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nuxt/**", ".output/**", "dist/**", "build/**"]
    },
    "clean": {
      "cache": false,
      "dependsOn": ["^clean"]
    },
    "//#clean:root": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"],
      "persistent": true
    },
    "lint": {
      "cache": false,
      "dependsOn": ["^lint"]
    },
    "//#lint:root": {
      "cache": false
    },
    "lint:fix": {
      "cache": false,
      "dependsOn": ["^lint:fix"]
    },
    "//#lint:root:fix": {
      "cache": false
    }
  }
}
